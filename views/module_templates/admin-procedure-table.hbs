<thead>
    <tr>
        <th style="background-color: #b8b6b6; cursor: pointer;"><i class="sort icon"></i>&nbsp;&nbsp;Name</th>
        <th class="center aligned" style="background-color: #b8b6b6;"> </th>
    </tr>
</thead>
<tbody class="fixed sticky">
    {{#each procedure}}
        <tr>
            <td>{{processname}}</td>
            <td class="right aligned">
                <button class="ui button" data-id="{{_id}}" data-name="{{processname}}" id="{{processname}}-edit">Edit</button>
                <button class="negative ui button" data-id="{{_id}}" data-name="{{processname}}" id="{{processname}}-delete">Delete</button>
            </td>
        </tr>
    {{/each}}
</tbody>